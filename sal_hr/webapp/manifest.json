{
    "_version": "1.32.0",
    "sap.app": {
        "id": "com.sal.salhr",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainService": {
                "uri": "/odata/v2/HumanResourceService/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            },
            "sfsfService": {
                "uri": "/odata/",
                "type": "OData"
            },
            "ds_timezones": {
				"uri": "model/timezones.json",
				"type": "JSON"
			}
        },
        "crossNavigation": {
            "inbounds": {
                "com-sal-salhr-inbound": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "HR_semantic",
                    "action": "display",
                    "title": "{{flpTitle}}",
                    "subTitle": "{{flpSubtitle}}",
                    "icon": ""
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "config": {
            "fullWidth": true
        },
        "rootView": {
            "viewName": "com.sal.salhr.view.App",
            "type": "XML",
            "async": true,
            "id": "flexibleColumnLayout"
        },
        "dependencies": {
            "minUI5Version": "1.97.2",
            "libs": {
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.sal.salhr.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "TwoWay",
                    "defaultCountMode": "Inline",
                    "refreshAfterChange": true,
                    "useBatch": false,
                    "defaultUpdateMethod": "PUT"
                },
                "dataSource": "mainService",
                "preload": true
            },
            "timezonesData": {
                "type": "sap.ui.model.json.JSONModel",
				"dataSource": "ds_timezones",
                "preload": true
            },
            "sfsfModel": {
                "type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "sfsfService",
                "preload": false
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.f.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.sal.salhr.view",
                "controlId": "flexibleColumnLayout",
                "controlAggregation": "pages",
                "clearControlAggregation": false,
                "transition": "slide"
            },
            "routes": [
                {
                    "pattern": "masterView",
                    "name": "master",
                    "target": [
                        "master"
                    ]
                },
                {
                    "pattern": "detail/{parentMaterial}/{layout}",
                    "name": "detail",
                    "target": [
                        "master",
                        "detail"
                    ]
                },
                {
                    "pattern": "detail/{parentMaterial}/detailDetail/{childModule}/{layout}",
                    "name": "detailDetail",
                    "target": [
                        "master",
                        "detail",
                        "detailDetail"
                    ]
                },
                {
                    "pattern": "RaiseRequest/{parentMaterial}/{layout}",
                    "name": "RaiseRequest",
                    "layout": "EndColumnFullScreen",
                    "titleTarget": "",
                    "greedy": false,
                    "target": [
                        "RaiseRequest"
                    ]
                },
                {
                    "pattern": "LeaveRequest/{parentMaterial}/{layout}",
                    "name": "LeaveRequest",
